class Solution {
public:
    int minimumTotal(vector<vector<int>>& triangle) {
        int row = triangle.size();
        int dp[row][row];
        memset(dp, 0, sizeof(int) * row * row);
        for(int i = 0; i < row; i++)
            dp[row-1][i] = triangle[row-1][i];
        for(int j = row - 2; j >=0; j--) {
            for(int i = 0; i < j + 1;  i++)
            dp[j][i] = min()
        }
        }
};
